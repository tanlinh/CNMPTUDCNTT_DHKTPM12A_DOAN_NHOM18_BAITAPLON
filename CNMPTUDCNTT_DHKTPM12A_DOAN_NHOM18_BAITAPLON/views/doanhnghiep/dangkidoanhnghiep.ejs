<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Đăng kí tài khoản doanh nghiệp</title>

    <link rel="shortcut icon" type="image/png" href="images/icontitle.jpg"/>
    <link rel="stylesheet" href="css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="css/animate.css">

    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <link rel="stylesheet" href="css/owl.theme.default.min.css">
    <link rel="stylesheet" href="css/magnific-popup.css">

    <link rel="stylesheet" href="css/aos.css">

    <link rel="stylesheet" href="css/ionicons.min.css">

    <link rel="stylesheet" href="css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="css/jquery.timepicker.css">


    <link rel="stylesheet" href="css/flaticon.css">
    <link rel="stylesheet" href="css/icomoon.css">
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/style2.css">
    <!-- Main css -->
    <link rel="stylesheet" href="css/style1.css">
</head>

<body>

    <div class="main">

        <section class="signup">
            <!-- <img src="images/signup-bg.jpg" alt=""> -->
            <div class="container">
                <div class="signup-content">
                    <form method="POST" id="signup-form" class="signup-form">
                        <h2 class="form-title">Đăng kí tài khoản doanh nghiệp</h2>
                        <div class="form-group">
                            <input type="text" class="form-input" name="email" id="email"
                                placeholder="Nhập tên doanh nghiệp" />
                        </div>
                        <div class="form-group">
                            <label>Chọn các dịch vụ</label>
                            <table class="table table-striped">
                                <tr>
                                    <th>Mã dịch vụ</th>
                                    <th>Tên dịch vụ</th>
                                    <th>Thao tác</th>
                                </tr>
                                <tr>
                                    <Td>001</Td>
                                    <Td>Tiền điện</Td>
                                    <td><a href="#" class="loginhere-link">Xóa</a></td>
                                </tr>
                                <tr>
                                    <Td>002</Td>
                                    <Td>Tiền Nước</Td>
                                    <td><a href="#" class="loginhere-link">Xóa</a></td>
                                </tr>
                                <tr>
                                    <td>
                                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalCart">Thêm dịch vụ</button>
                                        <div class="modal fade" id="modalCart" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                            <div class="modal-dialog" role="document">
                                                <div class="modal-content">
                                                    <!--Header-->
                                                    <div class="modal-header">
                                                            <h4 class="modal-title" id="myModalLabel">Thêm dịch vụ</h4>
                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                              <span aria-hidden="true">×</span>
                                                            </button>
                                                    </div>

                                                    <div class="modal-body">
                                                            <div class="form-group">
                                                                    <input type="text" class="form-input" name="name" id="name" placeholder="Nhập mã dịch vụ" />
                                                            </div>
                                                            <div class="form-group">
                                                                    <input type="text" class="form-input" name="name" id="name" placeholder="Nhập tên dịch vụ" />
                                                            </div>
                                                           
                                                            <a href="#" class="btn btn-primary">Thêm</a>
                                                        </div>
                                                </div>
                                            </div>
                                        </div>                
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-input" placeholder="Nhập địa chỉ" />

                        </div>
                        <div class="form-group">
                            <input type="text" class="form-input" placeholder="Nhập Email" />
                        </div>

                        <div class="form-group">
                            <input type="text" class="form-input" name="re_password" id="re_password"
                                placeholder="Nhập số điện thoại" />
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-input" name="re_password" id="re_password"
                                placeholder="Nhập số Fax" />
                        </div>
                        <div class="form-group">
                                <input type="text" class="form-input" name="re_password" id="re_password"
                                    placeholder="Nhập Username" />
                            </div>
                            <div class="form-group">
                                    <input type="password" class="form-input" name="re_password" id="re_password"
                                        placeholder="Nhập Password" />
                                </div>
                        <div class="form-group">
                            <input type="submit" name="submit" id="submit" class="form-submit" value="Đăng kí" />
                        </div>
                    </form>
                    <p class="loginhere">
                        Bạn đã có tài khoản? <a href="../dangnhap" class="loginhere-link">Đăng nhập tại đây</a>
                        Hoặc hủy?
                        <a href="/" class="loginhere-link">Thoát</a>
                    </p>
                </div>
            </div>
    </div>
    </div>
    </section>

    </div>

    <!-- JS -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="js/jquery.min.js"></script>
