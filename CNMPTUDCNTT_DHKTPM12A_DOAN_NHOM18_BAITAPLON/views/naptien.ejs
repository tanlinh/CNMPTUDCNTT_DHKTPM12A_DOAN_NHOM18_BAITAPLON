              <span class="icon mb-3 d-block flaticon-neurology"></span>
              <h2 class="mb-4" style="text-align: center">Nạp tiền</h2>
              <form action="/savenaptien" method="GET">
                <td><input type="hidden" name="id" value="<%=id%>" style="width:300px; height: 30px"></td>
                      <td><input type="hidden" name="ten" value="<%=ten%>" style="width:300px; height: 30px"></td>
                <table border="0" class="table table-borderless">
                  <tr>
            
                    <td>Nhập số tiền muốn nạp: </td>
                    <td><input type="number" min="10000" max="100000000" name="sotien" style="width:300px; height: 30px"></td>
                  </tr>
                  <tr>
                    <td></td>
                      <td><button style="width: 150px;height:50px" class="btn btn-info" type="submit">Nạp tiền</button></td>
                      
                  </tr>
                </table> 
               </form> 
            </div>

            <div class="tab-pane fade py-5" id="v-pills-fitness" role="tabpanel" aria-labelledby="v-pills-fitness-tab">
                <span class="icon mb-3 d-block flaticon-cardiogram"></span>
                <h2 class="mb-4" style="text-align: center">Lịch sử giao dịch</h2>
                <table class="table">
                        <thead class="thead-dark">
                          <tr>
                            <th scope="col">Mã hóa đơn đã thanh toán</th>
                            <th scope="col">Tên dịch vụ</th>
                            <th scope="col">Tên doanh nghiệp</th>
                            <th scope="col">Ngày thanh toán</th>
                            <th scope="col">Tên khách hàng</th>
                            <th scope="col">Số tiền</th>
                          </tr>
                        </thead>
                        <!--Load database tại đây-->
                        <tbody>
                          <%lichsu.forEach((ls)=>{%>

                            <tr>
                              <td><%=ls.idhd%></td>
                              <td><%=ls.tendv%></td>
                              <td><%=ls.tendn%></td>
                              <td><%=ls.ngaylap%></td>
                              <td><%=ls.tenkh%></td>
                              <td><%=ls.sotien%> VND</td>
                            </tr>
                            <%})%>
                        </tbody>
                      </table>  
            </div>

            <div class="tab-pane fade py-5" id="v-pills-reception" role="tabpanel" aria-labelledby="v-pills-reception-tab">
                <span class="icon mb-3 d-block flaticon-neurology"></span>
                <h2 class="mb-4" style="text-align: center">Vấn tin số dư</h2>
                <form>
                  <table border="0" class="table table-borderless">
                    <tr>
                          <table border="0" class="table table-striped" >
                                  <tr>
                                    <Th>Số tiền hiện có</Th><th>Số tiền đã nạp</th><th> Số tiền đã chi</th>
                                    <!--Xu lý tại đây-->
                                  </tr>
                                   
                                 <tr><td><%=sodu%> VND</td><td><%=sotiennap%> VND</td><td><%=thanhtoan%> VND</td></tr>   
                                  
                                
                          </table> 
            </div>

