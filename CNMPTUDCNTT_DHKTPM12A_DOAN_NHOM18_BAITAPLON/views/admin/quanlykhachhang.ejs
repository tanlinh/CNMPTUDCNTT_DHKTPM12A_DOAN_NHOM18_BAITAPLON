    <script> $('#myModal').on('shown.bs.modal', function () {
        $('#myInput').trigger('focus')
      }) 
      </script>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="index.html">
                            <img src="images/icon/logo.png" alt="CoolAdmin" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            
        </header>
        <!-- END HEADER MOBILE-->

        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="../admin1">
                    <img src="images/icon/logo.png" alt="Cool Admin" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                      
                        <li>
                            <a href="../admin1">
                                <i class="fas fa-chart-bar"></i>Quản lý doanh nghiệp</a>
                        </li>
                        <li>
                            <a href="../quanlykhachhang">
                                <i class="fas fa-table"></i>Quản lý khách hàng</a>
                        </li>
                        <li>
                            <a href="../quanlydichvu">
                                <i class="far fa-check-square"></i>Quản lý dịch vụ</a>
                        </li>
                        <li>
                            <a href="../themdichvu">
                                <i class="fas fa-map-marker-alt"></i>Thêm dịch vụ</a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <!-- <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button type="submit" class="form-control btn btn-primary">Search</button> -->
                            </form>
                            <div class="header-button">
                                <div class="noti-wrap">
                                    
                                    
                             
                                </div>
                                <div class="account-wrap">
                                 
                                        <div class="content">
                                            <a class="js-acc-btn" href="#">Xin chào Admin</a>    
                                            <a href="../" class="btn btn-danger">Đăng xuất</a>
                                        </div> 
                                   
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->

            <!-- Chèn nội dung Chart tại đây-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <span class="icon mb-3 d-block flaticon-cardiogram"></span>
                    <h2 class="mb-4" style="text-align: center">Quản lý thông tin khách hàng</h2>
                    <table class="table">
                        <thead class="thead-dark">
                            <tr>
                                <th scope="col">Mã khách hàng</th>
                                <th scope="col">Tên khách hàng</th>
                                <th scope="col">Chức vụ</th>
                                <th scope="col">Số điện thoại</th>
                                <th scope="col">CMND</th>
                                <th scope="col">Địa chỉ</th>
                                <th scope="col">Số dư</th>
                                <th scope="col">Số tiền nạp</th>
                                <th>Thao tác</th>
                            </tr>
                        </thead>
                        <!--Load database tại đây-->
                        <tbody>
                            <% qlkh.forEach((kh) =>{  %>

                            <tr>
                                <td><%=kh.id%></td>
                                <td><%=kh.ten%></td>
                                <td><%=kh.chucvu%></td>
                                <td><%=kh.sdt%></td>
                                <td><%=kh.cmnd%></td>
                                <td><%=kh.diachi%></td>
                                <td><%=kh.sodu%></td>
                                <td><%=kh.sotiennap%></td>
                                <td><a href="./updatekhachhang?id=<%=kh.id%>&&&ten=<%=kh.ten%>" class="loginhere-link">sửa</a></td>
            
                            </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>


    <!-- Jquery JS-->
    <script src="vendor/jquery-3.2.1.min.js"></script>
